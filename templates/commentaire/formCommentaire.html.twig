{% extends 'base.html.twig' %}

{% block body %}
	<h1 class="text-center m-5">Formulaire de commentaire</h1>
	<div class="container">
		{{form_start(form)}}
		{{form_row(form.auteur)}}
		{{form_row(form.commentaire)}}
		{{form_row(form.note)}}
		{{form_row(form.categorie)}}


		<div class="text-center">
			<button class="btn btn-primary m-5" type="submit">
				Ajouter
			</button>
		</div>

		{{form_end(form)}}


	</div>

{% endblock %}
